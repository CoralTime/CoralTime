{
  "manifestVersion": 1,
  "id": "CoralTimeTracker",
  "version": "0.0.6.36",
  "publisher": "CoralTimeExtDev",
  "name": "CoralTime tracker",
  "description": "Track your time in CoralTime",
  "public": false,
  "icons": {
    "default": "static/img/logo.png"
  },
  "categories": [
    "Plan and track"
  ],
  "tags": [
    "work",
    "track time"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "repository": {
      "uri": "https://github.com/CoralTime/CoralTime"
    },
    "issues": {
      "uri": "https://github.com/CoralTime/CoralTime/issues"
    },
    "support": {
      "uri": "https://coralteq.com"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/CoralTime/CoralTime"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.work"
  ],
  "contributions": [
    {
      "id": "CoralTimeTracker",
      "type": "ms.vss-work-web.work-item-form-control",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "CoralTimeTracker",
        "description": "Track your time in CoralTime.",
        "previewImageUrl": "static/img/logo.png",
        "uri": "static/coraltime-index.html",
        "order": 99,
        "height": 500,
        "supportedScopes": [
          "project_team"
        ]
      }
    },
    {
      "id": "ConfigurationDialogButton",
      "type": "ms.vss-web.action",
      "targets": [
        "ms.vss-work-web.work-item-context-menu"
      ],
      "properties": {
        "text": "CoralTime Configuration",
        "name": "CoralTimeTracker Configuration",
        "description": "Configuration dialog button.",
        "uri": "static/coraltime-configuration.html",
        "icon": "static/img/logo-menu.png"
      }
    },
    {
      "id": "ConfigurationDialog",
      "type": "ms.vss-web.control",
      "description": "CoralTimeTracker Configuration dialog.",
      "properties": {
        "uri": "static/coraltime-configuration.html"
      }
    }
  ],
  "files": [
    {
      "addressable": true,
      "path": "static/coraltime-index.html"
    },
    {
      "addressable": true,
      "path": "static/coraltime-configuration.html"
    },
    {
      "addressable": true,
      "path": "static/css"
    },
    {
      "addressable": true,
      "path": "static/img"
    },
    {
      "addressable": true,
      "path": "static/lib"
    },
    {
      "path": "dist",
      "addressable": true
    },
    {
      "addressable": true,
      "path": "src"
    }
  ]
}
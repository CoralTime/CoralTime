{
  "manifestVersion": 1,
  "id": "CoralTimeTracker",
  "version": "1.0.0",
  "publisher": "CoralTimeExtDev",
  "name": "CoralTime tracker",
  "description": "Track your time in CoralTime",
  "public": false,
  "icons": {
    "default": "static/img/logo.png"
  },
  "categories": [
    "Plan and track"
  ],
  "tags": [
    "work",
    "track time"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "repository": {
      "uri": "https://github.com/CoralTime/CoralTime"
    },
    "issues": {
      "uri": "https://github.com/CoralTime/CoralTime/issues"
    },
    "support": {
      "uri": "https://coralteq.com"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/CoralTime/CoralTime"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "CoralTimeTracker",
      "type": "ms.vss-work-web.work-item-form-control",
      "targets": [
        "ms.vss-work-web.work-item-form",
        ".CoralTimeTracker-Configuration"
      ],
      "properties": {
        "name": "CoralTimeTracker",
        "description": "Track your time in CoralTime.",
        "previewImageUrl": "static/img/logo.png",
        "uri": "static/coraltimetrackerindex.html",
        "order": 99,
        "height": 500,
        "supportedScopes": [
          "project_team"
        ]
      }
    },
    {
      "id": "CoralTimeTracker-Configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [
        "ms.vss-dashboards-web.widget-configuration"
      ],
      "properties": {
        "name": "CoralTimeTracker Configuration",
        "description": "Configures the CoralTimeTracker.",
        "uri": "static/coraltimetrackerconfiguration.html"
      }
    }
  ],
  "files": [
    {
      "addressable": true,
      "path": "static/coraltimetrackerindex.html"
    },
    {
      "addressable": true,
      "path": "static/coraltimetrackerconfiguration.html"
    },
    {
      "addressable": true,
      "path": "static/css"
    },
    {
      "addressable": true,
      "path": "static/img"
    },
    {
      "addressable": true,
      "path": "static/lib"
    },
    {
      "path": "dist",
      "addressable": true
    },
    {
      "addressable": true,
      "path": "src"
    }
  ]
}